<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<DialogPane prefHeight="224.0" prefWidth="305.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.trung.RemovalWarningBox">
   <header>
      <AnchorPane prefHeight="34.0" prefWidth="260.0">
         <children>
            <Label layoutX="14.0" prefHeight="34.0" prefWidth="101.0" text="Warning">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </header>
   <content>
      <AnchorPane prefHeight="170.0" prefWidth="264.0">
         <children>
            <Label layoutX="14.0" layoutY="39.0" prefHeight="38.0" prefWidth="192.0" text="Bạn có muốn xóa từ này không?" />
            <Button fx:id="acceptButton" layoutX="33.0" layoutY="125.0" mnemonicParsing="false" onAction="#processAcceptButton" prefHeight="25.0" prefWidth="48.0" text="Có" />
            <Button fx:id="turnToEditWordButton" layoutX="127.0" layoutY="125.0" mnemonicParsing="false" onAction="#processEditWordButton" text="Sửa từ" />
            <Button fx:id="cancelButton" layoutX="219.0" layoutY="125.0" mnemonicParsing="false" onAction="#closeStage" text="Không" />
         </children>
      </AnchorPane>
   </content>
</DialogPane>
